   <%- include("header") -%>
    <div class="box" id="heading">
    <h1><%=dday%></h1>
    </div>

    <div class="box">
       <% for(let i=0;i<newItemList.length;i++){ %>
        <div class="item">
        <input type="checkbox">
        <p><%= newItemList[i] %></p>
        </div>
    <%}%>
    <form class= "item"action="/" method="post">
        <input type="text" name="newItem" placeholder="New item" autocomplete="off">
        <button type="submit" name="list" value="<%=dday%>">+</button>
       </form>
    </div>
    <%- include("footer") -%>
